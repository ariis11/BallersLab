{
  "name": "ballerslab-backend",
  "version": "1.0.0",
  "description": "Backend API for BallersLab authentication and data",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "debug": "nodemon --inspect=9229 src/index.js",
    "build": "prisma generate",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "node src/seed.js",
    "postinstall": "prisma generate",
    "start:prod": "prisma migrate deploy && node src/index.js"
  },
  "dependencies": {
    "@prisma/client": "^5.10.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "cron": "^3.5.0",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.4.0",
    "express": "^4.18.0",
    "express-rate-limit": "^7.1.0",
    "express-validator": "^7.2.1",
    "helmet": "^7.1.0",
    "joi": "^17.12.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.0",
    "prisma": "^5.10.0"
  },
  "keywords": [
    "authentication",
    "api",
    "postgresql",
    "prisma"
  ],
  "author": "BallersLab",
  "license": "MIT"
}
